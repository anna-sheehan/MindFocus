{
    "name": "MindFocus",
    "description": "Human Computer Interaction Group Project",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["alarms", "notifications", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_title": "MindFocus",
      "default_popup": "homepage.html"
    },
    "icons": {
      "16": "/images/icons8-brain-16.png",
      "32": "/images/icons8-brain-32.png",
      "48": "/images/icons8-brain-48.png",
      "128": "/images/icons8-brain-100.png"
    }
  }
  